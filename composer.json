    "autoload": {
        "psr-4": {
            "Melasistema\\HydeLayoutsManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luca Visciola",
            "email": "info@melasistema.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "hyde/framework": "^1.7"
